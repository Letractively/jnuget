        <updated>2011-11-24T10:52:58Z</updated>
        <author>
            <name>Dennis Doomen, Martin Opdam</name>
        </author>
        <link rel="edit-media" title="Package" href="Packages(Id='FluentAssertions',Version='1.6.0')/$value" />
